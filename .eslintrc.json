{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:cypress/recomended"],
    "rules": {
        "cypress/no-unnecessary-waiting": "warn",
        "cypress/no-assigning-return-values": "error",
        "cypress/no-async-tests": "error"
    }
}